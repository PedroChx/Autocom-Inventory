---
import Layout from '../layouts/Layout.astro';
import Label from '../components/Label.astro'
---

<Layout title="Autocom">
	<Label/>
</Layout>

<style>
	html{
		background-image: url("../img/cover.jpg");
		background-repeat: no-repeat;
		background-size: 100%;
	}

	main {
		margin: auto;
		padding: 1rem;
		width: 800px;
		max-width: calc(100% - 2rem);
		color: rgb(5, 0, 0);
		font-size: 20px;
		line-height: 1.6;
	}
</style>
